<% if @team.errors.any? %>
  <div id="error_explanation">
    <h2>
      <%= pluralize(@team.errors.count, "error") %>
      prohibited this post from being saved:
    </h2>
 
    <ul>
    <% @team.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>
<%=form_for @team do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%=collection_select(:team, :members, User.all, :id, :name, prompt: true)%>
<br>
#<%= fields_for @team.members do |member| %>
#  Member name: <%= member.text_field :name %>
#  <br>
#<% end %>
<%=f.submit%>
<%end%>